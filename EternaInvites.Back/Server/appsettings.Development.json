{
  "ConnectionStrings": {
    "DefaultConnection": "Server={{DB_SERVER}};Port={{DB_PORT}};Database={{DB_DATABASE}};Uid={{DB_USER}};Pwd={{DB_PASSWORD}};SslMode={{DB_SSL_MODE}};AllowPublicKeyRetrieval={{DB_ALLOW_PUBLIC_KEY_RETRIEVAL}};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
